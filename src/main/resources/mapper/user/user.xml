<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.dasom.task.kakaoPay.repository.user.UserRepository">

    <select id="findById" parameterType="java.lang.String"
            resultType="com.dasom.task.kakaoPay.model.user.User">
        SELECT user_no, id, user_name, member_id
        FROM user
        where id = #{id};
    </select>
</mapper>

